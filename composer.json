{
    "name": "astrotomic/laravel-mime",
    "type": "library",
    "description": "Laravel wrapper for symfony/mime",
    "keywords": [
        "astrotomic",
        "laravel-mime"
    ],
    "homepage": "https://astrotomic.info",
    "license": "MIT",
    "authors": [
        {
            "name": "Tom Witkowski",
            "email": "gummibeer@astrotomic.info",
            "homepage": "https://gummibeer.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "illuminate/support": "^6.0 || ^7.0",
        "symfony/mime": "^5.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0 || ^5.0",
        "phpunit/phpunit": "^8.0 || ^9.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Astrotomic\\LaravelMime\\MimeTypesServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Astrotomic\\LaravelMime\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Astrotomic\\LaravelMime\\Tests\\": "tests"
        }
    },
    "support": {
        "email": "dev@astrotomic.info",
        "issues": "https://github.com/Astrotomic/laravel-mime/issues",
        "source": "https://github.com/Astrotomic/laravel-mime"
    }
}
